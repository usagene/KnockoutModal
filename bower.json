{
  "name": "KnockoutModal",
  "description": "KnockoutModal is a wrapper around bootstrap modal dialog, which works as a knockout binding.",
  "version": "1.0.0-alpha",
  "keywords": [
    "modal",
    "js",
    "bootstrap",
    "knockout",
    "dialog"
  ],
  "homepage": "https://github.com/usagene/modal",
  "main": [
    "dist/*.*"
  ],
  "ignore": [
    ".*",
    "_config.yml",
    "CNAME",
    "composer.json",
    "CONTRIBUTING.md",
    "docs",
    "js/tests",
    "test-infra"
  ],
  "dependencies": {
    "jquery": ">= 1.9.0",
    "bootstrap": "~3.2.0",
    "requirejs": "~2.1.14",
    "knockoutjs": "~3.2.0",
    "jqueryui": "~1.11.1"
  }
}
